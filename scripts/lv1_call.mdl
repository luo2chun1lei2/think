# 描述基本的程序调用关系。
# 看看可以不可以简单写
# a --xxx--> b #用箭头表示的通用关系。
# xxx("A", "B") # 一种用函数表是的通用的关系
# ("B", C) # 默认的关系，关系名字就C的名字。
# has("B", D) # 包含关系

Model name=mdl

# 元素区域
Element name="Main.cpp"
Element name="main()"

Element name="Process.cpp"
Element name="class Process"
Element name="class ProcessCmdLine"
Element name="ProcessCmdLine()"
Element name="exec()"

Element name="Model.cpp"
Element name="class Model"
Element name="Model(name)"

# 关系区域

Relation name="container" from="Main.cpp" to="main()"
Relation name="container" from="Process.cpp" to="ProcessCmdLine()"
Relation name="container" from="Model.cpp" to="class Model"

Relation name="inherit" from="class ProcessCmdLine" to="class Process"

Relation name="own" from="class ProcessCmdLine" to="ProcessCmdLine()"
Relation name="own" from="class ProcessCmdLine" to="exec()"

Relation name="use" from="main()" to="ProcessCmdLine()"
Relation name="use" from="main()" to="exec()"

Relation name="has" from="class ProcessCmdLine" to="class Model"

Relation name="own" from="class Model" to="Model(name)"
Relation name="use" from="exec()" to="Model(name)"


#option=text
Output name=out